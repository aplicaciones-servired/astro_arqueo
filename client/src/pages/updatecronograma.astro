---
import Layout from "@/layouts/Layout.astro";
import { CalendarioCronograma } from "@/components/CalendarioCronograma";
---

<Layout title="Calendario de Cronogramas">
  <main class="container mx-auto px-4 py-8">
    <div class="mb-6">
      <h1 class="text-3xl font-bold text-gray-800">Calendario de Cronogramas</h1>
      <p class="text-gray-600 mt-2">Vista mensual de los arqueos programados por punto de venta</p>
    </div>
    <CalendarioCronograma client:load />
  </main>
</Layout>
