{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "module": "esnext",
  "moduleResolution": "node",
  "target": "esnext",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "ignoreDeprecations": "5.0",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
