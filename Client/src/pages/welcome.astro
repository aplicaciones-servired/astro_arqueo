---
import Arqueo from "../components/Arqueos";
import Layout from "../layouts/Layout.astro";
import { getSession } from "auth-astro/server";

const session = await getSession(Astro.request);

if (!session) {
  return Astro.redirect("/");
}

---

<Layout>
  <!-- component -->
  <Arqueo client:load />
</Layout>
